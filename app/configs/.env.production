# Production Configuration
APP_NAME=prophet-agent
APP_ENV=production
HTTP_PORT=8080

# Google Cloud
GOOGLE_CLOUD_PROJECT=temple-square
GOOGLE_CLOUD_LOCATION=global

# Database (Cloud SQL via Unix socket)
DB_DIALECT=postgres
DB_HOST=/cloudsql/temple-square:us-west1:temple-square-db
DB_PORT=5432
DB_USER=postgres
DB_NAME=conference
DB_SSL_MODE=disable
# DB_PASSWORD from Secret Manager

# Assets
ASSETS_BUCKET=temple-square-assets
ASSETS_BASE_URL=https://storage.googleapis.com/temple-square-assets

# Domain
DOMAIN=templesquare.dev
